{
  "name": "form-ark",
  "version": "1.0.0-dev.11",
  "description": "",
  "author": "ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/form-ark.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/form-ark/issues"
  },
  "homepage": "https://github.com/ux-ui-pro/form-ark",
  "sideEffects": false,
  "scripts": {
    "serve": "vite",
    "clean": "rimraf dist",
    "build": "vite build",
    "lint:ts": "eslint src/**/*.ts",
    "lint:fix:ts": "eslint src/**/*.ts --fix",
    "format:ts": "prettier --write src/**/*.ts",
    "lint:css": "stylelint src/**/*.scss",
    "lint:fix:css": "stylelint src/**/*.scss --fix",
    "lint:fix": "yarn lint:fix:ts && yarn format:ts && yarn lint:fix:css"
  },
  "source": "src/index.ts",
  "main": "dist/core.cjs.js",
  "module": "dist/core.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/core.es.js",
      "require": "./dist/core.cjs.js"
    },
    "./textfield": {
      "import": "./dist/textfield.es.js",
      "require": "./dist/textfield.cjs.js"
    },
    "./dropdown": {
      "import": "./dist/dropdown.es.js",
      "require": "./dist/dropdown.cjs.js"
    },
    "./checkable": {
      "import": "./dist/checkable.es.js",
      "require": "./dist/checkable.cjs.js"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@rollup/plugin-terser": "0.4.4",
    "@types/node": "24.1.0",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "sass": "1.89.2",
    "stylelint": "16.23.0",
    "stylelint-config-standard-scss": "15.0.1",
    "stylelint-selector-bem-pattern": "4.0.1",
    "rimraf": "6.0.1",
    "typescript": "5.8.3",
    "vite": "7.0.6",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-sass-dts": "1.3.31"
  },
  "keywords": []
}
